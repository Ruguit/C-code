#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<String.h>
int main() {
	char ch[100]= {};
	scanf("%s",&ch);
	int len = strlen(ch);
	int i = 0;
	int Eglish = 0;
	int KongGe=0;
	int Number = 0;
	int QiTa = 0;
	while (i < len) {
		if (ch[i] >= 'a'&&ch[i] <= 'z'|| ch[i] >= 'A'&&ch[i] <= 'Z')Eglish += 1;
		else if (ch[i] >= '0'&&ch[i] <= '9')Number += 1;
		else if (ch[i] == 32)KongGe += 1;
		else	QiTa += 1;
		i++;
	}
	printf("%d,%d,%d,%d",Eglish,KongGe,Number,QiTa);
	return 0;
}	-
